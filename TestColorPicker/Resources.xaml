    <SolidColorBrush x:Key="CalendarDayTodayTextBrush" Color="{StaticResource NormalTextColor}"/>
    <SolidColorBrush x:Key="CalendarSelectedTextBrush" Color="{StaticResource NormalTextColor}"/>
    <SolidColorBrush x:Key="CalendarDayTodayBackgroundBrush" Color="{StaticResource CalendarDayTodayBackground}"/>
    <SolidColorBrush x:Key="CalendarDaySelectedBackgroundBrush" Color="{StaticResource ContainerBackgroundColor}"/>
    <SolidColorBrush x:Key="CalendarDayHoverBrush" Color="{StaticResource InactiveItemBackColor}"/>
    <SolidColorBrush x:Key="CalendarDayBlackoutBrush" Color="{StaticResource CalendarDayBlackoutColor}"/>
    <SolidColorBrush x:Key="CalendarDayBlackoutEllipseBrush" Color="{StaticResource ButtonBorderColor}"/>
    <SolidColorBrush x:Key="CalendarButtonSelectedBrush" Color="{StaticResource DarkContrastColor}"/>
    <SolidColorBrush x:Key="CalendarButtonHoverBrush" Color="{StaticResource InactiveItemBackColor}"/>
    <SolidColorBrush x:Key="CalendarButtonSelectedBorderBrush" Color="{StaticResource LightContrastColor}"/>
    <SolidColorBrush x:Key="CalendarNavigateBrush" Color="{StaticResource NormalTextColor}"/>
    <SolidColorBrush x:Key="CalendarNavigateHoverBrush" Color="{StaticResource LightContrastColor}"/>
    <!--<SolidColorBrush x:Key="CalendarDisabledBrush" Color="#C1272727"/>-->
    <Rectangle x:Key="CalendarShape" RadiusX="1" RadiusY="1"/>
    <!--#endregion-->

    <!--#region Menu and context menu -->
    <Color x:Key="MenuSeparatorColor">#FF333337</Color>
    <Color x:Key="MenuItemHoverColor">#FF333334</Color>

    <Color x:Key="MenuAndDropdownBackgroundColor">#FF3C3F41</Color>
    <!--new-->
    <SolidColorBrush x:Key="MenuItemTextColorBrush" Color="{StaticResource NormalTextColor}"/>
    <!--***********-->
    <SolidColorBrush x:Key="MenuBackgroundBrush" Color="{StaticResource WindowBackgroundColor}"/>
    <SolidColorBrush x:Key="MenuItemBackgroundBrush" Color="{StaticResource MenuAndDropdownBackgroundColor}"/>
    <SolidColorBrush x:Key="MenuItemIconBackgroundBrush" Color="{StaticResource MenuAndDropdownBackgroundColor}"/>
    <SolidColorBrush x:Key="MenuBorderBrush" Color="{StaticResource MenuSeparatorColor}"/>
    <SolidColorBrush x:Key="MenuSeparatorBrush"  Color="{StaticResource MenuSeparatorColor}"/>
    <SolidColorBrush x:Key="MenuBackgroundHoverBrush"  Color="{StaticResource MenuItemHoverColor}"/>
    <SolidColorBrush x:Key="MenuSelectedBrush" Color="{StaticResource MenuItemHoverColor}"/>
    <SolidColorBrush x:Key="MenuSelectedTextBrush" Color="{StaticResource NormalTextColor}"/>
